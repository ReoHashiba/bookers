<div class="create">
    <h2>New book</h2>
    <!--form with部分-->
    <%= form_with model: @book do |f| %>
        <!--tile入力-->
        <div class="title">
            <label>Title</label>
            <%= f.text_field :title %>
        </div>
        <!--body入力-->
        <div class = "body">
            <label>Body</label>
            <%= f.text_area :body %>
        </div>
        
        <%= f.submit 'Create Book' %>
    <% end %>
</div>